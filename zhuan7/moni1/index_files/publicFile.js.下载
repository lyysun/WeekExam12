// 神策页面加载时间
var start_time = new Date();
//定义起始时间
var end_time = "";
//定义结束时间
window.onload = function () {
    end_time = new Date();
    zpStat.track('pageLoadTime', {
        loadTime: end_time.getTime() - start_time.getTime()
    })
}